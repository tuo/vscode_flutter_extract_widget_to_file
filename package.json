{
  "name": "flutter-extract-widget-to-file",
  "displayName": "Flutter Extract Widget to File",
  "description": "Extracts a Flutter widget and creates a file to use it.",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "Tuo Huang",
  "icon": "images/icon_flat.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuo/vscode_flutter_extract_widget_to_file.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.extractFlutterWidgetToFile",
        "title": "Extract Flutter Widget to File"
      }
    ],
    "keybindings": [
      {
        "command": "extension.extractFlutterWidgetToFile",
        "key": "shift+ctrl+e",
        "mac": "shift+cmd+e"
      }
    ],
    "languages": [
      {
        "id": "dart",
        "extensions": [".dart"],
        "aliases": ["Dart"],
        "filenames": [],
        "configuration": "./language-configuration.json"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.extractFlutterWidgetToFile",
          "group": "navigation",
          "when": "editorLangId == 'dart'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0",
    "@types/mkdirp": "^0.3.29"
  },
  "dependencies": {  
    "mkdirp": "^0.5.1"
  }
}
